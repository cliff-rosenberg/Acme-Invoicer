<table class="table">
  <thead>
    <tr>
      <th scope="col">Invoice ID</th>
      <th scope="col">Invoice Date</th>
    </tr>
  </thead>
  <tbody>
    {{#each rendered as |Invoice| }}
    <tr>
      <th scope="row">{{Invoice.customer_id}}</th>
      <td>{{Invoice.invoice_date}}</td>
    </tr>
    {{/each}}
    </tbody>
</table>
